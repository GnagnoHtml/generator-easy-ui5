{
  "env": {
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 2019
  },
  "rules": {
    "block-scoped-var": 1,
    "keyword-spacing": 2,
    "space-unary-ops": 2,
    "camelcase": 1,
    "consistent-return": 1,
    "no-warning-comments": 1,
    "default-case": 1,
    "no-console": 2,
    "no-unused-vars": 2,
    "no-trailing-spaces": 2,
    "no-underscore-dangle": 0,
    "quotes": [
      1,
      "single"
    ],
    "key-spacing": [
      1,
      {
        "beforeColon": false
      }
    ],
    "comma-spacing": [
      1,
      {
        "before": false,
        "after": true
      }
    ],
    "no-shadow": 2,
    "no-irregular-whitespace": 2
  }
}